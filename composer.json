{
    "name": "atournayre/framework",
    "description": "A framework and helpers on top of Symfony",
    "type": "library",
    "keywords": [
        "framework",
        "dev"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "AurÃ©lien Tournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.22",
        "aimeos/map": "^3.12",
        "doctrine/collections": "^1.6 || ^2",
        "nesbot/carbon": "^2.72",
        "psr/log": "^1.1 || ^2.0 || ^3",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/event-dispatcher": "^6.4 || ^7.0",
        "symfony/filesystem": "^6.4 || ^7.0",
        "symfony/finder": "^6.4 || ^7.0",
        "symfony/http-foundation": "^6.4 || ^7.0",
        "symfony/mailer": "^6.4 || ^7.0",
        "symfony/options-resolver": "^6.4 || ^7.0",
        "symfony/process": "^6.4 || ^7.0",
        "symfony/routing": "^6.4 || ^7.0",
        "symfony/stopwatch": "^6.4 || ^7.0",
        "symfony/string": "^6.4 || ^7.0",
        "symfony/translation": "^6.4 || ^7.0",
        "symfony/twig-bridge": "^6.4 || ^7.0",
        "symfony/uid": "^6.4 || ^7.0",
        "twig/twig": "^2.13|^3.0.4",
        "webmozart/assert": "^1.11",
        "nyholm/psr7": "^1.8",
        "archtechx/enums": "^1.1",
        "symfony/messenger": "^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Atournayre\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atournayre\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "atournayre/phpstan-elegant-object": "^0.2.0",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.1",
        "rector/swiss-knife": "^0.2.16",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-doctrine": "^1.4",
        "phpstan/phpstan-phpunit": "^1.4",
        "phpstan/phpstan-strict-rules": "^1.6",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "spaze/phpstan-disallowed-calls": "^3.4",
        "tomasvotruba/unused-public": "^0.3.9",
        "tomasvotruba/lines": "^0.5.1",
        "phpstan/extension-installer": "^1.4",
        "phparkitect/phparkitect": "^0.3.33",
        "friendsofphp/php-cs-fixer": "^3.59"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "^6.4 || ^7.0"
        }
    }
}
