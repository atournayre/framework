{
    "name": "atournayre/framework",
    "description": "A framework and helpers on top of Symfony",
    "type": "library",
    "keywords": [
        "framework",
        "dev"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "AurÃ©lien Tournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 || ^8.0",
        "symfony/string": "^5.4",
        "webmozart/assert": "^1.11",
        "aimeos/map": "^3.7",
        "doctrine/collections": "^1.6",
        "symfony/finder": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/console": "^5.4",
        "psr/log": "^1.1 || ^2.0",
        "symfony/filesystem": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/process": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/options-resolver": "^5.4",
        "nesbot/carbon": "^2.72",
        "symfony/routing": "^5.4",
        "symfony/http-foundation": "^5.4",
        "twig/twig": "^2.13|^3.0.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/mailer": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "symfony/uid": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Atournayre\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atournayre\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.1",
        "rector/swiss-knife": "^0.2.16",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-doctrine": "^1.4",
        "phpstan/phpstan-phpunit": "^1.4",
        "phpstan/phpstan-strict-rules": "^1.6",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "spaze/phpstan-disallowed-calls": "^3.4",
        "tomasvotruba/unused-public": "^0.3.9",
        "tomasvotruba/lines": "^0.5.1",
        "phpstan/extension-installer": "^1.4",
        "phparkitect/phparkitect": "^0.3.33",
        "friendsofphp/php-cs-fixer": "^3.59"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "5.4.*"
        }
    }
}
